<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kuba.statmaster_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DruzynyActivity"
            android:label="@string/title_activity_druzyny"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.MainActivity" />
        </activity>
        <activity
            android:name=".RozgrywkiActivity"
            android:label="@string/title_activity_rozgrywki"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.MainActivity" />
        </activity>
        <activity
            android:name=".DodajTeamActivity"
            android:label="@string/title_activity_dodaj_team"
            android:parentActivityName=".DruzynyActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.DruzynyActivity" />
        </activity>
        <activity
            android:name=".ZawodnikActivity"
            android:label="@string/title_activity_zawodnik"
            android:parentActivityName=".DodajTeamActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.DodajTeamActivity" />
        </activity>
        <activity
            android:name=".ZawodnicyListActivity"
            android:label="@string/title_activity_zawodnicy_list"
            android:parentActivityName=".DruzynyActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.DruzynyActivity" />
        </activity>
        <activity
            android:name=".Wybierz5_Activity"
            android:label="@string/title_activity_wybierz5_"
            android:parentActivityName=".RozgrywkiActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.RozgrywkiActivity" />
        </activity>
        <activity
            android:name=".StatisticActivity"
            android:label="@string/title_activity_statistic"
            android:parentActivityName=".RozgrywkiActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kuba.statmaster_app.Wybierz5_Activity" />
        </activity>
    </application>

</manifest>